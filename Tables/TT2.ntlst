DEF TRUTH_TABLE2
  PORT IN A
  PORT IN B
  PORT IN C
  PORT OUT Z
  
  NET ANDAC
  NET iB

  INST NOT1 NOT B iB
  INST AND1 AND A C ANDAC
  INST OR1 OR ANDAC iB Z
ENDDEF




DEF TRUTH_TABLE2
  PORT IN A
  PORT IN B
  PORT IN C
  PORT OUT Z
  
  NET iAC

  INST GN1 NAND A C iAC
  INST GN2 NAND iAC B Z
ENDDEF